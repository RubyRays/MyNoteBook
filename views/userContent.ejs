<%- include("partials/header"); -%>
<%-include('partials/nav.ejs')-%>


<div class="userContent-style">
<% newPage.forEach(function(newPageContent){%>
    <% if(newPageContent._id == pageEntry){%>
    <div class="container-fluid">
    <h1 class="page-entry full-title">
        <%=newPageContent.title%>
    </h1>
    <p class="page-entry full-body">
        <%=newPageContent.content%>
    </p>
    </div>
    <%}%>
    <% }) %>
<div class="back-btn">
    <a class="btn btn-light" href="/pages" role="button"><i class="fas fa-home"></i></a>
</div>
</div>
    <%- include("partials/footer"); -%>